<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
	<words>
		<% @words.each do |word| %>
		<word>
			<id><%= word.id %></id>
			<english><%= word.word %></english>
			<translation><%= word.translation %></translation>
			<createdtime><%= word.created_at %></createdtime>
			<updatedtime><%= word.updated_at %></updatedtime>
			<sample><%= word.sample %></sample>
		</word>
  	   	<% end %>
	</words>
</rss>